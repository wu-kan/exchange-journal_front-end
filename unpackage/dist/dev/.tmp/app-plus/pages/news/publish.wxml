<view class="uni-page-body"><view class="feedback-body"><input class="feedback-input" placeholder="日记的标题是…" data-event-opts="{{[['input',[['__set_model',['$0','contact','$event',[]],['sendDate']]]]]}}" value="{{sendDate.contact}}" bindinput="__e"/></view><view class="feedback-title feedback-star-view"><text>今日心情</text><view class="feedback-star-view"><block wx:for="{{stars}}" wx:for-item="value" wx:for-index="key" wx:key="key"><text data-event-opts="{{[['tap',[['chooseStar',['$0'],[[['stars','',key]]]]]]]}}" class="{{['feedback-star',key<sendDate.score?'active':'']}}" bindtap="__e"></text></block></view></view><view class="feedback-body"><textarea class="feedback-textare" placeholder="请输入…" data-event-opts="{{[['input',[['__set_model',['$0','content','$event',[]],['sendDate']]]]]}}" value="{{sendDate.content}}" bindinput="__e"></textarea></view><choose vue-id="1" count="{{count}}" imgList="{{imgList}}" data-event-opts="{{[['^changes',[['fileChange']]]]}}" bind:changes="__e" bind:__l="__l"></choose><compress class="vue-ref" vue-id="2" maxwh="{{maxwh}}" quality="{{quality}}" data-ref="compress" bind:__l="__l"></compress><view class="swiper-list"><view class="uni-list-cell uni-list-cell-pd feedback-title"><view class="uni-list-cell-db ">压缩图片</view><switch checked="{{isYasuo}}" data-event-opts="{{[['change',[['changeIndicatorDots',['$event']]]]]}}" bindchange="__e"></switch></view></view><button class="feedback-submit" type="default" data-event-opts="{{[['tap',[['send',['$event']]]]]}}" bindtap="__e">提交</button></view>